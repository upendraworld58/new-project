/* 
    ##Device = Tablets, Ipads (portrait)
    ##Screen = B/w 768px to 1024px
  */

@media (min-width: 768px) and (max-width: 1024px) {
    .text-content {
        width: 90%;
        margin: auto;
    }
    .image-block {
        width: 40%;
    }
    .section {
        width: 100%;
        /* height: auto; */
    }
    .slider-nav,
    #home-banner-nav.slider-nav {
        position: inherit;
        margin: 30px auto;
        top: 0;
        right: 0;
    }
    .panel {
        padding: 120px 0 80px;
    }
}


/* 
    ##Device = Tablets, Ipads (landscape)
    ##Screen = B/w 768px to 1024px
  */

@media (min-width: 768px) and (max-width: 1024px) and (orientation: landscape) {
    .text-content {
        width: 80%;
        margin: auto;
    }
    .image-block {
        width: 80%;
    }
    .section {
        width: 100%;
        height: 100vh;
    }
}


/* 
    ##Device = Low Resolution Tablets, Mobiles (Landscape)
    ##Screen = B/w 481px to 767px
  */

@media (min-width: 481px) and (max-width: 767px) {
    h2 {
        font-size: 30px;
    }
    h4 {
        font-size: 20px;
    }
    p {
        font-size: 14px;
    }
    .text-content {
        width: 90%;
        margin: auto;
    }
    .image-block {
        width: 30%;
    }
    .section {
        width: 100%;
        height: auto;
    }
    .slider-nav,
    #home-banner-nav.slider-nav {
        position: inherit;
        margin: 30px auto;
        top: 0;
        right: 0;
    }
    .panel {
        padding: 80px 0;
    }
}


/* 
    ##Device = Most of the Smartphones Mobiles (Portrait)
    ##Screen = B/w 320px to 479px
  */
@media (max-width: 767px){
    .logo{
        margin: 0;
    }
    .footer{
        margin-top: 0;
    }
}
@media (max-width: 480px) {
    .slider-nav span{
        background-size: 40px;
    }
    h2 {
        font-size: 30px;
    }
    h4 {
        font-size: 20px;
    }
    p {
        font-size: 14px;
    }
    .text-content {
        width: 90%;
        margin: auto;
    }
    .image-block {
        width: 60%;
    }
    .home-banner .image-block {
        margin-top: 10px !important;
        width: 60%;
        margin: auto;
    }
    .section {
        width: 100%;
        height: auto;
    }
    .slider-nav,
    #home-banner-nav.slider-nav {
        position: inherit;
        margin: 30px auto;
        top: 0;
        right: 0;
    }
    .panel {
        padding: 80px 0 0 0;
    }
}