.slider-nav {
    position: absolute;
    z-index: 9;
}

.slider-nav span {
    display: inline-block !important;
    background-color: transparent;
    width: 4.375rem !important;
    height: 3.125rem !important;
    margin: 0 10px;
}

.slider-nav span:hover {
    cursor: pointer;
}

.slider-nav span.prev-btn {
    background-image: url("../images/prev.svg");
    background-repeat: no-repeat;
}

.slider-nav span.next-btn {
    background-image: url("../images/next.svg");
    background-repeat: no-repeat;
}

.slider-nav span.prev-btn:hover {
    background-image: url("../images/prev_hover.svg");
    background-repeat: no-repeat;
}

.slider-nav span.next-btn:hover {
    background-image: url("../images/next_hover.svg");
    background-repeat: no-repeat;
}

#home-banner-nav.slider-nav {
    top: 60%;
    right: 20%;
}

#case-study-nav.slider-nav {
    position: inherit;
    top: 0;
    right: 0;
}

.button-light {
